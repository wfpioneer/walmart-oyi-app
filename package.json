{
  "name": "OYI",
  "version": "4.10.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "TZ='America/Chicago' jest",
    "test:coverage": "TZ='America/Chicago' jest --collect-coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build-release": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "build-stage-release": "cd android && ./gradlew clean && ./gradlew assembleStagingRelease",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.3",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "^6.4.2",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-native-picker/picker": "^2.4.10",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/devtools": "^6.0.19",
    "@react-navigation/elements": "^1.3.4",
    "@react-navigation/material-top-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.2.0",
    "@redux-devtools/core": "^3.13.1",
    "@redux-devtools/extension": "^3.2.5",
    "@redux-saga/testing-utils": "^1.1.3",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "axios": "^1.5.0",
    "i18n-js": "^3.8.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.15",
    "mathjs": "^11.5.1",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "qs": "^6.11.0",
    "react": "18.2.0",
    "react-devtools": "^4.27.3",
    "react-devtools-core": "^4.27.2",
    "react-native": "^0.71.0",
    "react-native-action-sheet": "^2.2.0",
    "react-native-app-auth": "^6.4.3",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.3.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-flipper": "^0.209.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-localize": "^2.2.4",
    "react-native-modal": "^13.0.0",
    "react-native-pager-view": "^5.4.11",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "^2.14.1",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.19.0",
    "react-native-side-menu-updated": "^1.3.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-tab-view": "^3.3.4",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.2.0",
    "react-native-wm-barcode": "2.36.8-alpha",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-flipper": "^2.0.2",
    "redux-saga": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.14.6",
    "@react-native-community/eslint-config": "^3.0.1",
    "@rnx-kit/align-deps": "^2.2.4",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.0",
    "@types/i18n-js": "^3.8.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.191",
    "@types/qs": "^6.9.7",
    "@types/react": "18.2.18",
    "@types/react-native": "^0.71.0",
    "@types/react-native-side-menu": "^1.1.2",
    "@types/react-native-svg-charts": "^5.0.12",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-redux": "^7.1.25",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.73.7",
    "moment-timezone": "^0.5.40",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.5"
  },
  "relativeDependencies": {
    "react-native-flipper": "./react-native-flipper"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest/setup.js"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|@?react-navigation|@?gorhom)"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "globals": {
      "window": {}
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleNameMapper": {
      "^axios$": "<rootDir>/node_modules/axios/dist/node/axios.cjs"
    }
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "datetime-picker",
        "gestures",
        "jest",
        "modal",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg"
      ]
    }
  }
}
